@model EntityLayer.Concrete.SocialMedia

@{
    ViewData["Title"] = "EditSocialMedia";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<form asp-action="EditSocialMedia" asp-controller="SocialMedia" method="post" class="form">
    <div class="form-group">
        <label for="Title" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" placeholder="Name...">
    </div>
    <div class="form-group">
        <label for="Title" class="form-label">Url</label>
        <input type="text" class="form-control" asp-for="Url" placeholder="Url...">
    </div>
    <div class="form-group">
        <label for="Title" class="form-label">Icon</label>
        <input type="text" class="form-control" asp-for="Icon" placeholder="Icon...">
    </div>
    <div class="form-group">
        <label for="Title" class="form-label">Status</label>
        <input type="text" class="form-control" asp-for="Status" placeholder="Status...">
    </div>
    <button type="submit" class="btn btn-primary">Add</button>
    <a class="btn btn-light" href="/SocialMedia/Index">Cancel</a>
</form>


